{
  "name": "rinzler-engine",
  "version": "1.0.0",
  "description": "A parallel processing engine for the browser.",
  "main": "./dist/rinzler.umd.js",
  "module": "./dist/rinzler.esm.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint --color --ignore-path .gitignore \"**/*.js\" \"**/*.ts\"",
    "test": "tsc -p src && tsc -p worker-src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "komit $HUSKY_GIT_PARAMS",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitSquared/rinzler.git"
  },
  "author": "Gabriel SAILLARD <gabriel@saillard.dev> (https://gaby.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GitSquared/rinzler/issues"
  },
  "homepage": "https://github.com/GitSquared/rinzler#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "husky": "^4.3.0",
    "komit": "^1.0.5",
    "lint-staged": "^10.5.1",
    "rollup": "^2.33.3",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "nanoid": "^3.1.17"
  }
}
